# Database Configuration
# Local PostgreSQL
# DATABASE_URL=postgresql://carbonseed:password@localhost:5432/carbonseed

# Neon DB (Production)
DATABASE_URL=postgresql://[user]:[password]@[endpoint].neon.tech/carbonseed?sslmode=require

# JWT Configuration
# cmd : python -c "import secrets; print(secrets.token_hex(16))"

SECRET_KEY=your-secret-key-change-in-production-min-32-chars
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=1440

# Gemini API (Optional - for ML-powered insights)
GEMINI_API_URL=https://your-gemini-api-url.com

# Frontend URL (for CORS)
# Local: http://localhost:3000
# Production: https://your-app.pages.dev
FRONTEND_URL=http://localhost:3000
